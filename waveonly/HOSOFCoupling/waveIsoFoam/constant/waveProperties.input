FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      environmentalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

seaLevel	0.00;

// A list of the relaxation zones in the simulation. The parameters are given
// in <name>Coeffs below.
relaxationNames (inlet outlet);

// Name to use for initialisation
initializationName inlet;

// External forcing definition
externalForcing Grid2Grid;

// Coefficients needed for the external forcing
Grid2GridCoeffs
{
    waveType            Grid2Grid;

    Tsoft               0;

    referencePosition     (-7.06 -15 0);
    refDirection    (1 0 0);
    refTime         0.0;

    referenceTheta      0;

    Grid2GridInputFileName Grid2Grid.dict;
}

inletCoeffs
{
    waveType externalSource;

    Grid2GridInputFileName Grid2Grid.dict;

    relaxationZone
    {
        relaxationScheme Spatial;
        relaxationShape  Rectangular;

        relaxType   INLET;
        startX      (-3.5 -1 -3);
        endX        (-1.2 1 2);
        orientation      (1.0 0.0 0.0);
    }
};

outletCoeffs
{
    waveType externalSource;

    Grid2GridInputFileName Grid2Grid.dict;

    relaxationZone
    {
        relaxationScheme Spatial;
        relaxationShape  Rectangular;

        relaxType   OUTLET;
        startX      (1.2 -1 -3);
        endX        (3.5 1 2);
        orientation      (1.0 0.0 0.0);
    }
};



