seaLevel            0;

relaxationNames     ( inlet outlet );

initializationName  inlet;

externalForcing     Grid2Grid;

Grid2GridCoeffs
{
    waveType            Grid2Grid;

    Tsoft               0;

    referenceTime       0;

    referencePosition   (-7.06 -15 0);

    referenceTheta      0;

    Grid2GridInputFileName Grid2Grid.dict;
}

inletCoeffs
{
    waveType            externalSource;

    relaxationZone
    {
        relaxationScheme    Spatial;
        relaxationShape     Rectangular;
        relaxType           INLET;
        startX              ( -7.06 -1 -3 );
        endX                ( -2 1 2 );
        orientation         ( 1 0 0 );
    }
}

outletCoeffs
{
    waveType            externalSource;

    relaxationZone
    {
        relaxationScheme    Spatial;
        relaxationShape     Rectangular;
        relaxType           OUTLET;
        startX              ( 2 -1 -3 );
        endX                ( 9.86 1 2 );
        orientation         ( 1 0 0 );
    }
